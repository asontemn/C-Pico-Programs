 // 3/08/23 scope of varriable practice 
 
 // function examples 2.cpp : This file contains the 'main' function. Program execution begins and ends there.
//

#include <iostream>
using namespace std;

int z = 1;

int f1(int,int);


int main()
{
    int x = 10;
    int y = 20;
    // int z;

    f1(x, y);
    cout << " z = " << z << endl;
}

int f1(int a, int b) {

    cout << "1-> inside f1 z = " << z << endl;

    //int z;
    z = a + b;

    cout << "Inside f1 z = " << z << endl;
    return z;

}
